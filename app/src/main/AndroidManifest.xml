    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.O_ttae">



        <activity
            android:name="com.example.o_ttae.ui.register.RegisterPageActivity"
            android:exported="true" />
        <activity
            android:name="com.example.o_ttae.ui.login.LoginPageActivity"
            android:exported="true" />
        <activity
            android:name="com.example.o_ttae.ui.tutorial.OnBoardingActivity"
            android:exported="true"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.o_ttae.ui.MainActivity"
            android:exported="true">

        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="b76pnpi3mj" />


    </application>




